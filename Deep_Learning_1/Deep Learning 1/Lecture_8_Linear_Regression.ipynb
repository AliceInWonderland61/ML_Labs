        "import numpy as np\n",
        "a_and_b = np.array([3, 76]) # a and b\n",
        "data = np.array([[2, 81], [4, 93], [6, 91], [8, 97]])\n",
        "train_x = data[:, 0]\n",
        "train_y = data[:, 1]"
      ],
      "metadata": {
        "id": "fK6jdNRJekEL"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "mse = sum(((a_and_b[0] * train_x + a_and_b[1]) - train_y)**2)/len(train_x)\n",
        "print(mse)"
